<div class='mt-4'>
    <h2>Khóa học của tôi</h2>
    <table class='table'>
        <thead>
            <tr>
                <th scope='col'>#</th>
                <th scope='col'>Tên khóa học</th>
                <th scope='col'>Mô tả</th>
                <th scope='col'>Video Id</th>
                <th scope='col'>Thời gian tạo</th>

            </tr>
        </thead>
        <tbody>
            {{#each courses}}
                <tr>
                    <th scope='row'>{{sum @index 1}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.videoId}}</td>
                    <td>{{this.createdAt}}</td>
                    <td>
                        <a type='button' class='btn btn-link'>Cập nhật</a>
                        <a type='button' class='btn btn-link'>Chỉnh sửa</a>
                    </td>

                </tr>
            {{/each}}
        </tbody>
    </table>
</div>